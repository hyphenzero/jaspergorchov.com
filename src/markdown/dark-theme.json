{
  "colors": {
    "editor.foreground": "#D1D5DB"
  },
  "tokenColors": [
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#C4B5FD"
      }
    },

    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D1D5DB"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#FDA4AF"
      }
    },
    {
      "name": "Readwrite Aliases",
      "scope": ["variable.other.readwrite.alias"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D1D5DB"
      }
    },

    {
      "name": "Braces",
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#F9FAFB"
      }
    },
    {
      "name": "Property Variables",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#D1D5DB"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#C4B5FD"
      }
    },
    {
      "name": "Assignment Operators",
      "scope": ["keyword.operator.assignment"],
      "settings": {
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#99F6E4"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#FDA4AF"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#FDE68A"
      }
    },
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#FDA4AF"
			}
		},
    {
      "name": "Strings in Imports",
      "scope": ["meta.import string", "meta.import punctuation.definition.string"],
      "settings": {
        "foreground": "#E5E7EB"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
